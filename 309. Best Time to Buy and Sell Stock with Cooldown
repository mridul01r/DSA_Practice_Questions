309. Best Time to Buy and Sell Stock with Cooldown
Link - https://leetcode.com/problems/best-time-to-buy-and-sell-stock-with-cooldown/description/
Difficulty  - Medium

Solution 1 - Recursive Approach - O(2^n) -> TLE
class Solution {
    public int helper(int[] prices, int i, int buyed){
        if(i > prices.length - 1) return 0;
        if(buyed == 1){
            return Math.max(-prices[i] + helper(prices, i + 1, 0), helper(prices, i + 1, 1));
        }else return Math.max(prices[i] + helper(prices, i + 2, 1), helper(prices, i + 1, 0));
    }
    public int maxProfit(int[] prices) {
        int n = prices.length;
        return helper(prices, 0, 1);
    }
}

Solution 2 - Memoization - O(n * 2)
class Solution {
    int[][] memo;
    int profit = 0;
    public int helper(int[][] memo, int[] prices, int i, int buyed){
        if(i > prices.length - 1) return 0;
        if(memo[i][buyed] != -1) return memo[i][buyed];
        if(buyed == 1){
            profit = Math.max(-prices[i] + helper(memo, prices, i + 1, 0), helper(memo, prices, i + 1, 1));
        }else profit = Math.max(prices[i] + helper(memo, prices, i + 2, 1), helper(memo, prices, i + 1, 0));
        return memo[i][buyed] = profit;
    }
    public int maxProfit(int[] prices) {
        int n = prices.length;
        memo = new int[n][2];
        for(int[] m : memo) Arrays.fill(m, -1);
        
        return helper(memo, prices, 0, 1);
    }
}
