CodeForces
A. ASCII Art Contest

Solution
import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        int c = sc.nextInt();
        
        int max = Math.max(a, Math.max(b, c));
        int min = Math.min(a, Math.min(b, c));
        int sum = a + b + c;
        
        if((max - min) < 10){
            System.out.println("final " + (sum - (max + min)));
        }else{
            System.out.println("check again");
        }
    }
}
